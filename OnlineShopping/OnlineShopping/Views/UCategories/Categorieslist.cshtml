@model IEnumerable<OnlineShopping.Models.DB.Sp_categories_Details_Result>

@{
    ViewBag.Title = "Categorieslist";
}

<h2>Categorieslist</h2>
<table class="table">
    <tr>
      
        <th>
            @Html.DisplayNameFor(model => model.CategoriesName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Categoriesimage)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <a href="@Url.Action("Brandlist","UBrands",new{id=item.CategoriesID})">@item.CategoriesName</a>
        </td>
        <td>
            <img src="@Url.Content("~/Images/"+item.Categoriesimage)" style="width:100px; height:100px" />
        </td>
    </tr>
}

</table>
